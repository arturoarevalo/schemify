{
    "name": "schemify",
    "version": "2.0.0",
    "description": "An opinionated, minimalist, dependency-free but functional TypeScript framework for complex object schema validation, including nested objects, arrays and inheritance",
    "main": "dist/lib/index.js",
    "types": "dist/lib/index.d.ts",
    "scripts": {
        "test": "node dist/tests/index.js",
        "build": "rm -rf dist && ./node_modules/.bin/tsc -p .",
        "lint": "tslint --project .",
        "prepublish": "rm -rf dist && ./node_modules/.bin/tsc -p ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/arturoarevalo/schemify.git"
    },
    "keywords": [
        "schema",
        "validation",
        "typescript"
    ],
    "author": "Arturo Arevalo",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/arturoarevalo/schemify/issues"
    },
    "homepage": "https://github.com/arturoarevalo/schemify#readme",
    "devDependencies": {
        "@types/node": "^10.9.1",
        "tslint": "^5.11.0",
        "typescript": "^3.0.1"
    },
    "dependencies": {
        "reflect-metadata": "^0.1.12"
    }
}
